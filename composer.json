{
    "name": "pdf-combiner/app",
    "description": "PDF Combiner - Gabungkan beberapa halaman PDF menjadi satu halaman",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "setasign/fpdi": "^2.3",
        "setasign/fpdf": "^1.8"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
